    "reason-apollo": "https://github.com/GTDev87/reason-apollo#6bbcb0514f581b8fde861e6a5e5340bf41a83b13",
    "reason-apollo-hooks": "2.7.0",
    "reason-react": "0.7.0",
    "reason-react-update": "^1.0.0",
    "recharts": "1.0.0-beta.10",
    "subscriptions-transport-ws": "^0.9.14",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "babel-loader": "7",
    "babel-plugin-emotion": "^9.2.11",
    "babel-plugin-tailwind-components": "^0.5.10",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "bs-platform": "5.1.0-dev.2",
    "concurrently": "^3.5.0",
    "graphql_ppx": "^0.2.7",
    "razzle": "2.4.0",
