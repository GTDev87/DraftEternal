{
  "name": "razzle-examples-with-reason-react",
  "version": "2.0.0-alpha.4",
  "license": "MIT",
  "homepage": "https://gtdev87.github.io/r_o/",
  "scripts": {
    "start": "concurrently \"npm run js-watch\" \"npm run bsb-watch\"",
    "local-introspection": "rm -rf lib; send-introspection-query http://localhost:4001/api",
    "js-watch": "RAZZLE_APP_ENV=local razzle start",
    "bsb-watch": "bsb -make-world -w",
    "clean": "bsb -clean-world && rm -rf build",
    "build": "bsb -make-world && cross-env NODE_OPTIONS=--max_old_space_size=4096 RAZZLE_APP_ENV=staging razzle build",
    "travis": "bsb -make-world && razzle build && cp ./build/public/static/js/bundle.*.js ./build/public/static/js/bundle.js && cp ./build/public/static/css/bundle.*.css ./build/public/static/css/bundle.css && cp ./template/index.html ./build/public/",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.0",
    "@glennsl/bs-json": "^3.0.0",
    "apollo-cache-inmemory": "1.6.2",
    "apollo-client": "2.6.3",
    "apollo-link": "1.2.12",
    "apollo-link-context": "1.0.18",
    "apollo-link-error": "1.1.11",
    "apollo-link-http": "1.5.15",
    "apollo-link-ws": "1.0.18",
    "bootstrap": "4.0.0",
    "d3-array": "1.2.1",
    "domain-renormalizr": "^0.0.0",
    "emotion": "^9.2.12",
    "express": "4.16.2",
    "flexsearch": "^0.6.32",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "graphql-voyager": "1.0.0-rc.25",
    "http-proxy-middleware": "^0.19.0",
    "prop-types": "15.6.1",
    "query-string": "^6.2.0",
    "random-seed": "^0.3.0",
    "re-debouncer": "^0.0.1",
    "react": "16.8.6",
    "react-apollo": "2.5.8",
    "react-csv": "1.1.1",
    "react-dom": "16.8.6",
    "react-emotion": "^9.2.12",
    "react-fontawesome": "1.6.1",
    "react-icons": "3.7.0",
    "react-infinite-scroller": "^1.2.4",
